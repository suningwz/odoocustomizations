<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="sequence_account_receivable" model="ir.sequence">
            <field name="name">Receivable Accounts</field>
            <field name="code">receivable.account</field>
            <field name="padding">5</field>
            <field name="number_next">10000</field>
            <field name="number_increment">1</field>
            <field name="implementation">no_gap</field>
        </record>
        <record id="sequence_account_payable" model="ir.sequence">
            <field name="name">Payable Accounts</field>
            <field name="code">payable.account</field>
            <field name="padding">5</field>
            <field name="number_next">70000</field>
            <field name="number_increment">1</field>
            <field name="implementation">no_gap</field>
        </record>

    </data>
</odoo>
